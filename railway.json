{
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "phases": {
        "install": {
          "dependsOn": [],
          "cmds": ["npm ci"]
        },
        "build": {
          "dependsOn": ["install"],
          "cmds": ["npm run build"]
        },
        "start": {
          "dependsOn": ["build"],
          "cmds": ["npm run start"]
        }
      }
    }
  }
}
